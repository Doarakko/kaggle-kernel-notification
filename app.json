{
    "name": "Kaggle Kernel Notification",
    "description": "Notify new kaggle kernels to Slack",
    "repository": "https://github.com/Doarakko/kaggle-kernel-notification",
    "logo": "https://avatars0.githubusercontent.com/u/1336944",
    "keywords": [
        "kaggle",
        "kernel",
        "notification",
        "slack"
    ],
    "env": {
        "SLACK_WEBHOOK_URL": {
            "description": ""
        },
        "LINE_NOTIFY_TOKEN": {
            "description": ""
        },
        "POST": {
            "description": "slack or line",
            "value": "slack"
        },
        "COMPETITIONS_LIST": {
            "description": "slack or line",
            "value": "['', '']"
        },
        "KAGGLE_KEY": {
            "description": "kaggle key",
            "value": "your key"
        },
        "KAGGLE_USERNAME": {
            "description": "kaggle username",
            "value": "your username"
        }
    },
    "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-python"
        }
    ],
    "addons": [
        "scheduler"
    ]
}